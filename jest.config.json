{
  "preset": "jest-expo",
	"setupFiles": [
      "<rootDir>/jest/setup.ts"
    ],
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx"],
  "collectCoverage": true,
  "coverageReporters": ["lcov"],
  "coverageDirectory": "<rootDir>/jest/test-coverage",
  "coverageThreshold": {
    "global": {
      "lines": 0,
      "branches": 0,
      "functions": 0,
      "statements": 0
    }
  }
}
